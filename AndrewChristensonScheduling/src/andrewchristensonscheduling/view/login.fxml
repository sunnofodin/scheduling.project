<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="230.0" prefWidth="400.0" style="-fx-background-color: #000000; -fx-border-color: #000000;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="andrewchristensonscheduling.view.LoginController">
   <children>
      <Label fx:id="welcome" contentDisplay="CENTER" layoutX="12.0" layoutY="39.0" prefHeight="38.0" prefWidth="385.0" text="Welcome! Please log in:" textAlignment="CENTER" textFill="#fffbfc">
         <font>
            <Font name="Helvetica Bold" size="24.0" />
         </font>
      </Label>
      <AnchorPane layoutX="29.0" layoutY="83.0" prefHeight="97.0" prefWidth="336.0">
         <children>
            <TextField fx:id="usernameTF" layoutX="136.0" layoutY="9.0" prefHeight="27.0" prefWidth="183.0" />
            <PasswordField fx:id="passwordTF" layoutX="136.0" layoutY="50.0" prefHeight="27.0" prefWidth="183.0" />
            <Label fx:id="username" contentDisplay="RIGHT" layoutX="1.0" layoutY="9.0" prefHeight="27.0" prefWidth="128.0" text="Username:" textAlignment="RIGHT" textFill="#fffbfc">
               <font>
                  <Font name="Helvetica Bold" size="18.0" />
               </font>
            </Label>
            <Label fx:id="password" layoutY="50.0" prefHeight="27.0" prefWidth="128.0" text="Password:" textFill="#fffbfc">
               <font>
                  <Font name="Helvetica Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="quitButton" cancelButton="true" layoutX="258.0" layoutY="177.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="27.0" prefWidth="92.0" style="-fx-background-radius: 5em;" text="Quit" />
      <Button fx:id="loginButton" defaultButton="true" layoutX="161.0" layoutY="177.0" mnemonicParsing="false" onAction="#handleSave" prefHeight="27.0" prefWidth="92.0" style="-fx-background-radius: 5em;" text="Login" />
   </children>
</AnchorPane>
